{
  "name": "campus-online-frontend",
  "description": "FAC-UNB campus online frontend web app",
  "homepage": "https://campus.kunst.cloud/",
  "version": "0.0.0-alpha",
  "private": true,
  "scripts": {
    "clean": "rm -rf public .cache",
    "dev": "gatsby develop --port \"${PORT:-5000}\" --open -H 0.0.0.0",
